{
    "name": "patient-application",
    "version": "1.0.0",
    "description": "Patient application implemented in JavaScript",
    "scripts": {
        "start": "nodemon src/server.js",
        "watch": "nodemon src/server.js --watch src",
        "prestart": "node initServer.js",
        "prewatch": "node initServer.js",
        "lint": "eslint .",
        "api-doc": "cd ../../docs/API/ && npm start",
        "readme-doc": "cd ../../docs/ReadMe/readme-website && yarn run start"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=5"
    },
    "author": "Jathin",
    "license": "Apache-2.0",
    "dependencies": {
        "axios": "^0.27.2",
        "body-parser": "^1.19.0",
        "concurrently": "^5.2.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "fabric-ca-client": "^2.2.0",
        "fabric-client": "^1.4.10",
        "fabric-network": "^2.2.17",
        "form-data": "^4.0.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^7.1.0",
        "morgan": "^1.9.1",
        "nodemon": "^1.19.1",
        "redis": "^3.0.2"
    },
    "devDependencies": {
        "eslint": "^7.16.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-react": "^7.22.0"
    },
    "nodemonConfig": {
        "watch": ["src"],
        "ignore": ["**/upload/**"],
        "delay": 1000
    }
}
